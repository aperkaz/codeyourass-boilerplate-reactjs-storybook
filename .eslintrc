{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack-dev.config.babel.js"
      }
    }
  },
  "rules": {
    // https://github.com/eslint/eslint/pull/8081#pullrequestreview-24397955
    "import/no-extraneous-dependencies": 0
  }
}
